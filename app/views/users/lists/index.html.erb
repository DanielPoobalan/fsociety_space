<div class="container-fluid">
  <div class="row">
    <div class="col-md-4">
      <div class="well">
      
      </div>

    </div>
    <div class="col-md-8">

    </div>
</div>

<table id="example" class="display" width="100%" cellspacing="0">
      <thead>
        <tr>
            <th>Date</th>
            <th>Country Name</th>
            <th>Latitude</th>
            <th>Logitude</th>
        </tr>
    </thead>
    <% @landslides.each do |row| %>
    <tr>
      <td><%= row.date %></td>
      <td><%= row.countryname %></td>
      <td><%= row.latitude %></td>
      <td><%= row.longitude %></td>
    </tr>
  <% end %>
</table>

<script type="text/javascript">
  $(document).ready(function() {
    $('#example').DataTable({
      "lengthMenu": [ 50, 75, 100 ]
    });
} );

</script>